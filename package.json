{
  "name": "virtualfs",
  "version": "0.0.1",
  "description": "In-memory fs replacement",
  "keywords": [
    "fs",
    "memory"
  ],
  "author": "Roger Qiu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-virtualfs.git"
  },
  "main": "dist/VirtualFS.cjs.js",
  "module": "dist/VirtualFS.es.js",
  "scripts": {
    "test": "ava -v",
    "doc": "documentation build ./lib/** -f html -o ./doc",
    "rollup": "rollup --config",
    "build": "npm test && npm run doc && npm run rollup"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "engines": {
    "node": ">=6.11.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "bl": "^1.0.0",
    "documentation": "^5.2.2",
    "rollup": "^0.49.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "buffer": "^5.0.7",
    "errno": "^0.1.4",
    "path-browserify": "0.0.0",
    "readable-stream": "^2.3.3",
    "resource-counter": "^0.2.0"
  }
}
